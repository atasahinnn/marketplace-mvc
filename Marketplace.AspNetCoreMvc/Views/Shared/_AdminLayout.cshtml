                                        string flag = "/assets/media/svg/admin/231-libya.svg";

                                        if (Context.Request.Cookies["LocalizationAdmin"] != null)
                                        {
                                            var uiCulture = Context.Request.Cookies["LocalizationAdmin"].Split('|')[0];

                                            uiCulture = uiCulture.Substring(uiCulture.IndexOf('=') + 1, uiCulture.Length - uiCulture.IndexOf('=') - 1);


                                            if (uiCulture == "tr-TR")
                                            {
                                                flag = "/assets/media/svg/admin/218-turkey.svg";
                                            }
                                            else if (uiCulture == "en-US")
                                            {
                                                flag = "/assets/media/svg/admin/260-united-kingdom.svg";
                                            }
                                            else
                                            {
                                                flag = "/assets/media/svg/admin/231-libya.svg";
                                            }
                                        }


                                    }
                                    <div class="btn btn-icon btn-clean btn-dropdown btn-lg mr-1">
                                        <img class="h-20px w-20px rounded-sm" src="@flag" alt="">
                                    </div>
                                </div>
