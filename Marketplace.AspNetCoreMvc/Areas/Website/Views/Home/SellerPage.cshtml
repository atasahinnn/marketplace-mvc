    <script src="~/assets/js/global/website/main.js"></script>
    <script src="~/assets/js/global/website/navigation.js"></script>
    <script src="~/assets/js/global/website/slick.js"></script>
    <script src="~/assets/js/global/website/jquery.fancybox.js"></script>
    <script src="~/assets/js/global/website/smooth-scrollbar.js"></script>
    <script src="~/assets/js/global/website/custom.js"></script>

